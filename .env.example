# Example environment variables for ScarCoin AgentNet
# Fill these values with real credentials and addresses before running scripts

# JSON-RPC endpoint for your blockchain network (e.g., Infura or Alchemy)
RPC_URL=

# Private key for the ritual agent wallet (used by RitualTrigger)
RITUAL_AGENT_KEY=

# Address of the ScarCoin contract
SCARCOIN_ADDRESS=

# Address of the RitualRegistry contract
REGISTRY_ADDRESS=

# Address of the FaucetAgent contract/account
FAUCET_AGENT_ADDRESS=

# Address of the Vault Unlock Agent (if used)
VAULT_AGENT_ADDRESS=
